{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "activityadfdev"
		},
		"alpha02_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'alpha02'"
		},
		"work01_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'work01'"
		},
		"alpha02_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datacopysa002.dfs.core.windows.net/"
		},
		"work01_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datacopysa001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "activityadfdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"alpha02_accountKey": {
						"value": "[parameters('alpha02_accountKey')]"
					},
					"work01_accountKey": {
						"value": "[parameters('work01_accountKey')]"
					},
					"alpha02_properties_typeProperties_url": {
						"value": "[parameters('alpha02_properties_typeProperties_url')]"
					},
					"work01_properties_typeProperties_url": {
						"value": "[parameters('work01_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}